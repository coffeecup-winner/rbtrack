<% provide(:title, 'Sign in') %>
<h1>Sign in</h1>
<%= form_for(:session, url: sessions_path) do |form| %>
  <%= form.label :email %>
  <%= form.text_field :email %>
  <%= form.label :password %>
  <%= form.password_field :password %>
  <%= form.submit 'Sign in', class: 'btn btn-large btn-primary' %>
<% end %>
<p>Not registered yet? <%= link_to 'Sign up', signup_path %></p>